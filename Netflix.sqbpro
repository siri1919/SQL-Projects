></column_widths><filter_values/><conditional_formats/><row_id_formats/><display_formats/><hidden_columns/><plot_y_axes/><global_filter/></table></browse_table_settings></tab_browse><tab_sql><sql name="SQL 1">/* To see or view the titles where the view count is more than 2 of a user */
SELECT Title, count(Title)
FROM Netflix_Data
WHERE Profile_Name = &quot;Kalyan&quot;
GROUP BY Title 
HAVING count(Title) &gt; 2

/* To find Device wise views/count of a user */
SELECT Device_Type, count(Title)
FROM Netflix_Data
WHERE Profile_Name = &quot;Shivani&quot;
GROUP BY Device_Type
ORDER BY COUNT(Title) DESC

/* To find the count of Title by video type wise of users */
SELECT Profile_Name,Supplement_Video_Type, count(Title)
FROM Netflix_Data
WHERE Profile_Name = &quot;Gaali&quot; OR Profile_Name = &quot;Kalyan&quot;
GROUP BY Supplement_Video_Type

/* To find the view counts of users */
SELECT Profile_Name, COUNT(Title)
FROM Netflix_Data
GROUP BY Profile_Name
ORDER BY COUNT(Title) desc


